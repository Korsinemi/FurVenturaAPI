{
    "version": 2,
    "builds": [
        {
            "src": "./src/app.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "./src/app.js",
            "methods": [ "GET", "PUT", "PATCH", "POST", "DELETE", "OPTIONS" ],
            "headers": {
                "Access-Control-Allow-Origin": "*"
            }
        }
    ]
}